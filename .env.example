# ChainOverflow - Production Environment Template

# IPFS Pinning Service (e.g. Pinata)
VITE_PINATA_API_KEY=your_api_key_here
VITE_PINATA_SECRET_KEY=your_secret_key_here

# IPFS Provider: 'mock' or 'pinata'
VITE_IPFS_PROVIDER=mock

# Pinata API Settings (Only needed if provider is 'pinata')
VITE_PINATA_JWT=
VITE_PINATA_GATEWAY=https://gateway.pinata.cloud

# Blockchain Configuration
VITE_NETWORK_ID=137 # Polygon Mainnet
VITE_CONTRACT_ADDRESS=0x...

# Indexing Layer (The Graph)
VITE_SUBGRAPH_URL=https://api.thegraph.com/subgraphs/name/...

# App Settings
VITE_APP_ENV=production
